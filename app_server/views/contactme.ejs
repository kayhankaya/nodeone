<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-8">
                    <div class="contact-panel">
                        <div class="card card-info">
                            <div class="card-header margin1em">
                                <div class="card-text">Contact Me</div>
                            </div>
                            <form style="margin:10px;">
                                <div class="row">
                                    <div class="col-sm-6 form-group">
                                        <input name="cname" type="text" class="form-control" placeholder="Your name">
                                    </div>
                                    <div class="col-sm-6 form-group">
                                        <input name="cemail" type="email" class="form-control" id="inputEmail4"
                                               placeholder="Email">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12 form-group">
                                        <input name="ctitle" type="text" class="form-control" placeholder="Title">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12 form-group">
                                    <textarea name="cmessage" type="text" rows="2" class="form-control"
                                              placeholder="Your message"></textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12 form-group text-right">
                                        <button type="submit" value="signup" class="btn btn-primary">Sign Up</button>
                                    </div>
                                </div>
                            </form>
                            <div class="card-footer">
                                <div class="text-right">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="contact-panel">
                        <div class="row">
                            <div class="col-sm-12">
                                <ul class="contact-icons">
                                    <li><i class="fa fa-map-marker fa-2x"></i>
                                        <p>San Francisco, CA 94126, USA</p>
                                    </li>

                                    <li><i class="fa fa-phone fa-2x"></i>
                                        <p>+ 01 234 567 89</p>
                                    </li>

                                    <li><i class="fa fa-envelope fa-2x"></i>
                                        <p>contact@mdbootstrap.com</p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>